   <children>
      <Button fx:id="encryptBtn" layoutX="262.0" layoutY="53.0" mnemonicParsing="false" onAction="#btnClick" text="Encrypt" />
      <TextField fx:id="fieldInput" layoutX="78.0" layoutY="53.0" prefHeight="25.0" prefWidth="175.0" promptText="Text to be encrypted" />
      <TextArea fx:id="encryptedTxt" editable="false" layoutX="7.0" layoutY="100.0" prefHeight="11.0" prefWidth="386.0" />
   </children>
</AnchorPane>
